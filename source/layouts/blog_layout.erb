<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Space Dolphin Labs Inc" %></title>

    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootswatch/3.3.1/spacelab/bootstrap.min.css">
    <%= stylesheet_link_tag 'spacedolphin' %>
    <%= javascript_include_tag  "all" %>
  </head>

  <body>
    <div class="container" id="main-container">
      <div class="row header-row">
          <img class="col-xs-2 col-md-1" src="images/spacedolphin_140w.png"> </img>

          <div class="col-xs-5 col-md-5">
            <h1> Space Dolphin Labs </h1>
          </div>
      </div>
      <%= yield %>
    </div>

    <footer class="footer">
      <div class="container">
        <div class="row footer-row">
          <img class="col-xs-2 col-md-1" src="/images/spacedolphin_140w.png"> </img>

          <div class="col-xs-5 col-md-5">
            <p class="text-muted">
              Space Dolphin Labs Inc  </br>
              802 NW 5th Avenue #100 </br>
              Gainesville, Florida, 32601
            </p>
          </div>

          <div class="col-xs-5">
            <p class="pull-right">
              email <a href="mailto:james@spacedolphin.io">james@spacedolphin.io</a><br/>
              phone (352) 389-5007 <br/>
            </p>
          </div>
      </div>
    </footer>
  </body>
</html>

